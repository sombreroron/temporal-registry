{
    "name": "temporal-registry",
    "version": "1.0.1",
    "description": "A TypeScript library for managing Temporal workflow and activity registries with metadata extraction and validation",
    "keywords": [
        "temporal",
        "workflow",
        "activity",
        "registry",
        "metadata",
        "typescript"
    ],
    "author": "David Ron, Shai Shwartz",
    "license": "MIT",
    "type": "commonjs",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "npm run clean && tsc -p tsconfig.lib.json",
        "build:watch": "tsc -p tsconfig.lib.json --watch",
        "clean": "rm -rf dist",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "prepublishOnly": "npm run build && npm test",
        "prepack": "npm run build",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sombreroron/temporal-registry.git"
    },
    "bugs": {
        "url": "https://github.com/sombreroron/temporal-registry/issues"
    },
    "homepage": "https://github.com/sombreroron/temporal-registry#readme",
    "dependencies": {
        "@babel/parser": "^7.23.0",
        "@babel/traverse": "^7.23.0",
        "@babel/types": "^7.23.0",
        "class-validator": "^0.14.2",
        "reflect-metadata": "^0.1.13",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@jest/test-sequencer": "^29.7.0",
        "@temporalio/worker": "^1.11.0",
        "@types/babel__traverse": "^7.20.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.0.0",
        "@whatwg-node/server": "^0.10.15",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "@temporalio/activity": "^1.0.0",
        "@temporalio/worker": "^1.0.0",
        "@whatwg-node/server": "^0.10.0"
    },
    "peerDependenciesMeta": {
        "@temporalio/worker": {
            "optional": true
        },
        "@temporalio/activity": {
            "optional": true
        },
        "@whatwg-node/server": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
